<html>
    <head>
        <title>All Queries</title>
        <%- include("../partial/Bootstrap.ejs")%>
    </head>
    <table>
        <thead>
            <tr>
                <th scope="col">Email Id</th>
                <th scope="col">Query</th>
            </tr>
        </thead>
        <tbody>
            <%
            var sno=1
            for (var i=0; i<result.length; i++){
                %>
                <tr>
                    <td><%=sno++%></td>
                    <td><%=result[i].email%></td>
                    <td><%=query</td>
                </tr>
                <%
            }
            %>
            }
        </tbody>
    </table>
</html>